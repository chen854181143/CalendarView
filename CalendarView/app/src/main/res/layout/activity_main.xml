<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.chenyang.calendarview.MainActivity">

    <com.chenyang.calendarview.MyCalendarView
        android:id="@+id/mycalendarview"
        xmlns:mycalendarview="http://schemas.android.com/apk/res/com.chenyang.calendarview"
        mycalendarview:dateFormat="MMMM yyyy"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.chenyang.calendarview.MyCalendarView>
</RelativeLayout>
